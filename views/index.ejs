    <div class="map">
       <img src="/tq/images/map.jpg" alt="">

      <div class="waypoint" id="w0" data-waypoint="" data-choice="65">A</div>
      <div class="waypoint" id="w1" data-waypoint="" data-choice="66">B</div>
      <div class="waypoint" id="w2" data-waypoint="" data-choice="67">C</div>
