      <div class="waypoint" id="w4" data-waypoint="" data-choice="69">E</div>
    </div>

    <div class="player" id="m1">
      <img src="/tq/images/mage-r.png" alt=""  >
      <div id="monster-bubble" class="talk-bubble    tri-right btm-right">
        <div class="talktext">
          <!-- <p class="taunt">You cannot defeat me!</p> -->
